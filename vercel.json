{
  "version": 2,
  "env": {
    "PORT": "@port",
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_USERNAME": "@db_username",
    "DB_PASSWORD": "@db_password",
    "DB_NAME": "@db_name",
    "AWS_REGION": "@aws_region",
    "AWS_BUCKET": "@aws_bucket",
    "AWS_ACCESS_KEY": "@aws_access_key",
    "AWS_SECRET_ACCESS_KEY": "@aws_secret_access_key"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}